{
    "apps": [
        {
            "name": "zca-bot",
            "script": "dist/index.js",
            "instances": 1,
            "autorestart": true,
            "watch": false,
            "time": true,
            "max_memory_restart": "500M",
            "env": {
                "NODE_ENV": "production"
            },
            "env_development": {
                "NODE_ENV": "development",
                "LOG_LEVEL": "debug"
            },
            "log_date_format": "YYYY-MM-DD HH:mm:ss",
            "merge_logs": true,
            "error_file": "logs/pm2-error.log",
            "out_file": "logs/pm2-output.log"
        }
    ]
}